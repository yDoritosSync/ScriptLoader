function TeleportPlayer4()
	local teleport_table = {
		location1 = Vector3.new(3002.07764, 316.000519, -3861.73853, -0.997963428, 0, 0.0637900159, 0, 1.00000012, -0, -0.0637900233, 0, -0.997963309), -- Primeiro Lugar
		location2 = Vector3.new() -- Segundo Lugar
	}
	
	local tween_s = game:GetService('TweenService')
	local tweeninfo = TweenInfo.new(4,Enum.EasingStyle.Linear) --Delay do Teleport
	
	local lp = game.Players.LocalPlayer
	
	function bypass_teleport(v)
		if lp.Character and 
		lp.Character:FindFirstChild('HumanoidRootPart') then
			local cf = CFrame.new(v)
			local a = tween_s:Create(lp.Character.HumanoidRootPart,tweeninfo,{CFrame=cf})
			a:Play()
			
		end
	end
	
	bypass_teleport(teleport_table.location1)	
end

--Tab


local AutoFarmTab = Window:MakeTab({
	Name = "Auto Farm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})



local Tab = Window:MakeTab({
	Name = "Money Tab",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local TeleportTab = Window:MakeTab({
	Name = "Teleport",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local DestroyerTab = Window:MakeTab({
	Name = "Destroyer",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})





--Toggles


Tab:AddToggle({
	Name = "Give me Money Bitch",
	Default = false,
	Callback = function(Value)
	    _G.AutoBuy = Value
		AutoBuy()
	end    
})


Tab:AddToggle({
	Name = "Drop Money Doritos-Kun",
	Default = false,
	Callback = function(Value)		
	      _G.AutoDrop = Value
		AutoDrop()
	end    
})


local Section = TeleportTab:AddSection({
	Name = "Places"
})


TeleportTab:AddButton({
	Name = "Kiribating Village",
	Callback = function()
      	TeleportPlayer()
  	end    
})


TeleportTab:AddButton({
	Name = "Zapiwara Mountain",
	Callback = function()
      	TeleportPlayer2()
  	end    
})


TeleportTab:AddButton({
	Name = "Demon Slasher",
	Callback = function()
      	TeleportPlayer3()
  	end    
})


TeleportTab:AddButton({
	Name = "Butterfly Mansion",
	Callback = function()
      	TeleportPlayer4()
  	end    
})



DestroyerTab:AddButton({
	Name = "Destroy UI",
	Callback = function()
      	DestroyUIs()
  	end    
})





